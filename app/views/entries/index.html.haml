%h1 Listing entries

%table
  %tr
    %th Contest
    %th User
    %th Description
    %th Data set url
    %th
    %th
    %th

  - @entries.each do |entry|
    %tr
      %td= entry.contest_id
      %td= entry.user_id
      %td= entry.description
      %td= entry.data_set_url
      %td= link_to 'Show', contest_entry_path(@contest, entry)
      %td= link_to 'Edit', edit_contest_entry_path(@contest, entry)
      %td= link_to 'Destroy', contest_entry_path(@contest, entry), :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Entry', new_contest_entry_path(@contest)
